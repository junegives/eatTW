<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid
                android:color="#F1F1F1"/>
            <corners
                android:radius="30dip"/>
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <solid android:color="#EAEAEA" />
            <corners android:radius="29dip" />
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape>
            <solid android:color="#E4E4E4" />
            <corners android:radius="28dip" />
        </shape>
    </item>

    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape>
            <solid android:color="#DDDDDD" />
            <corners android:radius="27dip" />
        </shape>
    </item>

    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="3dp">
        <shape>
            <solid android:color="#FFFFFF" />
            <corners android:radius="26dip" />
        </shape>
    </item>

    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="3dp">
        <shape>
            <solid android:color="#FFFFFF" />
            <corners android:radius="26dip" />
        </shape>
    </item>
</layer-list>